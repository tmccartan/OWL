    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasActivityObject -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasActivityObject">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity_Object"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasBall -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasBall">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Ball"/>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasActivityObject"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasBoard -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasBoard">
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Board"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#BoardGame"/>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasActivityObject"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasCard -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasCard">
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Card"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#CardGame"/>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasActivityObject"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasCompetitors -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasCompetitors">
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Player"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasLocation -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasLocation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Location"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasMembers -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasMembers">
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Player"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Team"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasPieces -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasPieces">
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Piece"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#PuzzleGame"/>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasActivityObject"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasPlayers -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasPlayers">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Player"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasTeam -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasTeam">
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Team"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasToken -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasToken">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#BoardGame"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Token"/>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasActivityObject"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasWinner -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasWinner">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Player"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#playersCardNumber -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#playersCardNumber">
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Card"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#CardGame"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#requiresAttribute -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#requiresAttribute">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Attributes"/>
    </ObjectProperty>
    


