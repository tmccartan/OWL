    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#numberOfActiveTeamPlayers -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#numberOfActiveTeamPlayers">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#TeamActivity"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </DatatypeProperty>
    


