    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Eating -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Eating">
        <equivalentClass>
