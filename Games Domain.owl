    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity">
        <rdfs:comment>A class representing an Activity</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity_Object -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity_Object">
        <rdfs:comment>A Class to represent objects that an activity uses</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Attributes -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Attributes">
        <rdfs:comment>A class that represents attributes of an activity</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Ball -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Ball">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity_Object"/>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#BasketBall -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#BasketBall">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Ball"/>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Basketball -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Basketball">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#PhysicalSport"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#TeamActivity"/>
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasBall"/>
                        <someValuesFrom rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#BasketBall"/>
                    </Restriction>
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasLocation"/>
                        <someValuesFrom rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Court"/>
                    </Restriction>
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#numberOfActiveTeamPlayers"/>
                        <maxCardinality rdf:datatype="&xsd;nonNegativeInteger">5</maxCardinality>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Blackjack -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Blackjack">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#CardGame"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#IndividualActivity"/>
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#playersCardNumber"/>
                        <onClass rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Card"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</qualifiedCardinality>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Board -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Board">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity_Object"/>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#BoardGame -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#BoardGame">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity"/>
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasBoard"/>
                        <onClass rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Board"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:comment>A class representing an Activity that requires a board</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Book -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Book">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity_Object"/>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Boxing -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Boxing">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#IndividualActivity"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#PhysicalSport"/>
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasLocation"/>
                        <someValuesFrom rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Ring"/>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Card -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Card">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity_Object"/>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#CardGame -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#CardGame">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Activity"/>
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasCard"/>
                        <someValuesFrom rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Card"/>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:comment>An activity that requires cards</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competition -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competition">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasCompetitors"/>
                        <onClass rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competitor"/>
                        <minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</minQualifiedCardinality>
                    </Restriction>
                    <Restriction>
                        <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasWinner"/>
                        <onClass rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competitor"/>
                        <maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</maxQualifiedCardinality>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:comment>A class that represents activities that have a winner </rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competitor -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Competitor">
