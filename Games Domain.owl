    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#IndividualActivity -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#IndividualActivity">
        <equivalentClass>
            <Restriction>
                <onProperty rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#hasTeam"/>
                <onClass rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Team"/>
                <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</qualifiedCardinality>
            </Restriction>
        </equivalentClass>
        <disjointWith rdf:resource="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#TeamActivity"/>
        <rdfs:comment>A class that requires players to be individual</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Jigsaw -->

    <Class rdf:about="http://www.semanticweb.org/ter/ontologies/2014/2/Games_Domain#Jigsaw">
        <equivalentClass>
